<Window x:Class="Epic.Hardware.FrameworkTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Epic.Hardware.FrameworkTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Orientation="Horizontal">
        <Button Content="WPF Xps" Width="120" Height="20" Margin="10" Click="WPFPrintXpsDocumentClick"/>
        <Button Content="WPF XpsStream" Width="120" Height="20" Margin="10" Click="WPFPrintXpsStreamClick"/>
        <Button Content="WPF XpsFile" Width="120" Height="20" Margin="10" Click="WPFPrintXpsFileClick"/>
        <Button Content="WPF XpsDialog" Width="120" Height="20" Margin="10" Click="WPFPrintXpsDialogClick"/>

        <Button Content="Process LPR" Width="120" Height="20" Margin="10" Click="ProcessLPRClick"/>


    </StackPanel>
</Window>
